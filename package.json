{
  "name": "@code-slice/log-parser",
  "version": "1.1.3",
  "description": "A CLI tool for analysing the IPs and URLs that appear in HTTP log files",
  "type": "module",
  "main": "bin/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./bin",
    "build:clean": "npm-run-all --silent clean build",
    "example": "node ./bin/index.js --file ./fixtures/example.log",
    "test": "vitest ./src/*"
  },
  "author": "Jarrod Membrey",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.2.0",
    "strip-ansi": "^7.0.1",
    "vitest": "^0.27.2",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/yargs": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.4"
  },
  "bin": {
    "parse-log": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jarrodjarrod/log-parser"
  },
  "homepage": "https://github.com/jarrodjarrod/log-parser"
}
